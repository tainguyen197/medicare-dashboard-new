{
  "name": "medicare-dashboard-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node -r dotenv/config src/db/migrate.js",
    "worker:dev": "wrangler dev src/worker/index.ts --local",
    "worker:deploy": "wrangler deploy src/worker/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run worker:dev\""
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250413.0",
    "@libsql/client": "^0.15.3",
    "@neondatabase/serverless": "^1.0.0",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.41.0",
    "hono": "^4.7.6",
    "next": "15.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "wrangler": "^4.10.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
